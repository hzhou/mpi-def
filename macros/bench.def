subcode: _autoload
    $include <sys/time.h>

subcode: time_start
    $local struct timeval t_start, t_finish
    gettimeofday(&t_start, 0)

subcode: time_finish(dur)
    gettimeofday(&t_finish, 0)

    $local double $(dur)
    $(dur) = (t_finish.tv_sec - t_start.tv_sec) + (double) (t_finish.tv_usec - t_start.tv_usec) * 1e-6
